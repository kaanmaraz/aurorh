<form method="post" action="{{ path('app_candidat_delete', {'id': candidat.id}) }}" onsubmit="return confirm('Êtes vous sûrs de vouloir supprimer ce candidat?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ candidat.id) }}">
    <button class="btn btn-danger"> <i class="fas fa-times"></i> Supprimer</button>
</form>
