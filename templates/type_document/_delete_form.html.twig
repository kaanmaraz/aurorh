<form method="post" action="{{ path('app_type_document_delete', {'id': type_document.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ type_document.id) }}">
    <button class="btn btn-danger"> <i class="fas fa-times"></i> Supprimer</button>
</form>
